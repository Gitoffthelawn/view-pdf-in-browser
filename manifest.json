{
  "manifest_version": 3,
  "name": "InlinePDF",
  "version": "1.0",
  "description": "Forces PDFs to be viewed inline in your browser.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "background": {
    "scripts": [
      "inline.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*/*.pdf"
  ]
}
